
const containerCalculator = document.createElement('div');
containerCalculator.classList.add('containerCalculator');
document.body.appendChild(containerCalculator);

const containerNumbers = document.createElement('div');
containerNumbers.classList.add('containerNumbers')
containerCalculator.appendChild(containerNumbers);

for (let i = 1; i <= 9; i++) {
    let numbers = document.createElement('button');
    numbers.classList.add('number');
    numbers.textContent = i;
    containerNumbers.appendChild(numbers);
};

